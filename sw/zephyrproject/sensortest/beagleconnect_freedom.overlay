&i2c0 {
	// Grove module defaults to 0x76
	// Environment Click defaults to 0x76
	environment: bme680@77 {
		compatible = "bosch,bme680";
		reg = <0x77>;
		label = "BME680-ENVIRONMENT";
	};

	airquality: sgp30@58 {
		compatible = "sensiron,sgp30";
		reg = <0x58>;
		label = "SGP30-AIRQUALITY";
	};
};
